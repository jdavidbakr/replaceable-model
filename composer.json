{
    "name": "jdavidbakr/replaceable-model",
    "type": "library",
    "description": "Adds 'REPLACE' and 'INSERT IGNORE' query capability to eloquent models",
    "keywords": [
        "jdavidbakr",
        "replaceable-model"
    ],
    "homepage": "https://github.com/jdavidbakr/replaceable-model",
    "license": "MIT",
    "authors": [
        {
            "name": "J David Baker",
            "email": "me@jdavidbaker.com",
            "homepage": "http://www.jdavidbaker.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/http-foundation": ">=5.0.7",
        "illuminate/support": "~5.2|^6.0|^7.0",
        "php" : ">=7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.4",
        "orchestra/testbench": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\ReplaceableModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\ReplaceableModel\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
