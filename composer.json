{
    "name": "jdavidbakr/replaceable-model",
    "type": "library",
    "description": "Adds 'REPLACE' and 'INSERT IGNORE' query capability to eloquent models",
    "keywords": [
        "jdavidbakr",
        "replaceable-model"
    ],
    "homepage": "https://github.com/jdavidbakr/replaceable-model",
    "license": "MIT",
    "authors": [
        {
            "name": "J David Baker",
            "email": "me@jdavidbaker.com",
            "homepage": "http://www.jdavidbaker.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1 || ^6.0",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "jdavidbakr\\ReplaceableModel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdavidbakr\\ReplaceableModel\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
